<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.bruce.model">
	<class name="IndexPic" table="t_index_pic">
		<id name="id" column="id">
			<generator class="identity"/>
		</id>
		<property name="title"/>
		<property name="subTitle" column="sub_title"/>
		<property name="linkType" column="link_type"/>
		<property name="linkUrl" column="link_url"/>
		<property name="newName" column="new_name"/>
		<property name="oldName" column="old_name"/>
		<property name="status"/>
		<property name="pos"/>
		<property name="createDate" column="create_date"/>
	</class>
</hibernate-mapping>